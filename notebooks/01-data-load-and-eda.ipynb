print(f"\n4. Extreme Event:")
extreme = df[df['median'] == df['median'].max()]
print(f"   - Maximum Kp occurred at: {extreme['Time (UTC)'].values[0]}")
print(f"   - Value: {extreme['median'].values[0]}")

print("\n" + "="*60)
